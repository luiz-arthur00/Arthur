10 INPUT "INSIRA A NOTA DE VE:"; VE
20 IF VE<0 OR VE>10 THEN PRINT "INSIRA A NOTA NOVAMENTE." : GOTO 10
30 INPUT "INSIRA A NOTA DE VC:"; VC
40 IF VC<0 OR VC>10 THEN PRINT "INSIRA A NOTA NOVAMENTE." : GOTO 30
50 LET AUX = (20-VE-VC)/2
60 IF AUX>4 THEN VF = AUX
70 IF AUX<=4 THEN VF = 4
80 PRINT "VF = "; VF
